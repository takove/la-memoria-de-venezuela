{
  "name": "memoria-venezuela-api",
  "version": "0.1.1",
  "description": "API for La Memoria de Venezuela - Venezuelan officials accountability database",
  "author": "Luis Rosal",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "pnpm typeorm migration:generate -d src/config/typeorm.config.ts",
    "migration:run": "pnpm typeorm migration:run -d src/config/typeorm.config.ts",
    "migration:revert": "pnpm typeorm migration:revert -d src/config/typeorm.config.ts",
    "seed:businesses": "ts-node -r tsconfig-paths/register src/scripts/seed-businesses.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/typeorm": "^10.0.1",
    "bullmq": "^5.66.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "fuzzball": "^2.2.3",
    "helmet": "^7.1.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.19",
    "wink-nlp": "^2.4.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.3",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "pnpm": {
    "overrides": {
      "qs": ">=6.14.1"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!main.ts",
      "!app.module.ts",
      "!**/*.module.ts",
      "!**/*.controller.ts",
      "!modules/ingestion/ingestion.controller.ts",
      "!modules/ingestion/ingestion.module.ts",
      "!modules/ingestion/ingestion.processor.ts",
      "!modules/ingestion/rss.service.ts",
      "!modules/ingestion/sample-data.ts",
      "!modules/ingestion/dedup/**",
      "!modules/ingestion/scoring/**",
      "!modules/ingestion/tier1/**",
      "!modules/ingestion/ner/wink-ner.service.ts",
      "!**/migrations/**",
      "!**/scripts/**",
      "!**/config/**"
    ],
    "coverageDirectory": "../coverage",
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 25,
        "lines": 60,
        "statements": 60
      }
    },
    "testEnvironment": "node"
  }
}
